<Window x:Class="Nikak.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nikak"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Nam Pizda" HorizontalAlignment="Center" FontSize="30" VerticalAlignment="Center"></TextBlock>
        <Image Source="Resources/maxresdefault.jpg" HorizontalAlignment="Left"></Image>
        <Grid Background="Yellow" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="Gray"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame"></Frame>
        <Button Content="Назад нахуй" Name="BtnBk" Width="100" Height="30" HorizontalAlignment="Right" Margin="10" Click="BtnBk_Click"></Button>
    </Grid>
</Window>
